    <panel name="autoresize" />
    <applet type="onLayoutFinish">
# this should be factored out into some helper code, but currently demonstrates applets.
from enigma import eSize, ePoint
orgwidth = self.instance.size().width()
orgpos = self.instance.position()
textsize = self["text"].getSize()
# y size still must be fixed in font stuff...
textsize = (textsize[0] + 60, textsize[1] + 60)
offset = 20
if self.type == self.TYPE_YESNO:
	offset = 60
wsizex = textsize[0] + 60
wsizey = textsize[1] + offset
if (1100 &gt; wsizex):
	wsizex = 1100
wsize = (wsizex, wsizey)
# resize
#self.instance.resize(eSize(*wsize))
# resize label
self["text"].instance.resize(eSize(*textsize))
# move list
listsize = (wsizex - 30, 605)
self["list"].instance.move(ePoint(185, int(textsize[1] + 180)))
self["list"].instance.resize(eSize(*listsize))
# center window
newwidth = wsize[0]
self.instance.move(ePoint(int(orgpos.x() + (orgwidth - newwidth) // 2), int(orgpos.y())))
	</applet>
